            android:layout_height="wrap_content" android:id="@+id/textViewCVC"
            app:layout_constraintHorizontal_bias="0.773" app:layout_constraintVertical_bias="0.374"/>
    <TextView
            android:text="카드번호"
            android:textColor="@color/연두색1"
            android:textStyle="bold"
            android:textSize="13dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView72"
            app:layout_constraintHorizontal_bias="0.103" app:layout_constraintVertical_bias="0.265"/>
    <TextView
            android:text="유효기한"
            android:textColor="@color/연두색1"
            android:textStyle="bold"
            android:textSize="13dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView73"
            app:layout_constraintHorizontal_bias="0.103" app:layout_constraintVertical_bias="0.32"/>
    <TextView
            android:text="CVC"
            android:textColor="@color/연두색1"
            android:textStyle="bold"
            android:textSize="13dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView74"
            app:layout_constraintHorizontal_bias="0.103" app:layout_constraintVertical_bias="0.375"/>
    <androidx.recyclerview.widget.RecyclerView
            android:layout_height="300dp"
            android:layout_weight="1" android:id="@+id/CardRecyclerView"
            android:layout_width="350dp"
            android:background="@color/white"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.767" app:layout_constraintHorizontal_bias="0.488"/>


</androidx.constraintlayout.widget.ConstraintLayout>