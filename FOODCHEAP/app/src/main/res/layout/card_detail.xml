            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.492" app:layout_constraintVertical_bias="0.471"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/view7"/>
