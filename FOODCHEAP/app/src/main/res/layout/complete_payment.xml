<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools" android:background="@color/white"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent">

    <View
            android:layout_width="340dp"
            android:layout_height="2px"
            android:background="#60000000"
            android:layout_gravity="center"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.49" app:layout_constraintVertical_bias="0.607"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/view4"/>
    <View
            android:layout_width="340dp"
            android:layout_height="2px"
            android:background="#60000000"
            android:layout_gravity="center"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.49" app:layout_constraintVertical_bias="0.748"
            app:layout_constraintEnd_toEndOf="parent" android:id="@+id/view5"/>
    <ImageView
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="100dp"
            android:layout_height="100dp" app:srcCompat="@drawable/checked_basket" android:id="@+id/imageView12"
            app:layout_constraintHorizontal_bias="0.498" app:layout_constraintVertical_bias="0.249"/>
    <TextView
            android:text="결제가 정상적으로 완료되었습니다."
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="20dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView34"
            app:layout_constraintHorizontal_bias="0.494" app:layout_constraintVertical_bias="0.401"/>
    <TextView
            android:text="가맹점명"
            android:textColor="#80000000"
            android:textSize="17dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView62"
            app:layout_constraintHorizontal_bias="0.115" app:layout_constraintVertical_bias="0.652"/>
    <TextView
            android:text="쿠키네 식료품"
            android:textColor="@color/black"
            android:textSize="17dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView64"
            app:layout_constraintHorizontal_bias="0.877" app:layout_constraintVertical_bias="0.651"/>
    <TextView
            android:text="상세 주문정보 보기"
            android:textColor="@color/연두색"
            android:textSize="15dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView66"
            app:layout_constraintHorizontal_bias="0.498" app:layout_constraintVertical_bias="0.804"/>
    <TextView
            android:text="0,000원"
            android:textColor="@color/연두색1"
            android:textSize="21dp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/TotalPrice3"
            app:layout_constraintHorizontal_bias="0.884" app:layout_constraintVertical_bias="0.715"/>
    <TextView
            android:text="결제금액"
            android:textColor="#80000000"
            android:textSize="17dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/textView63"
            app:layout_constraintHorizontal_bias="0.115" app:layout_constraintVertical_bias="0.714"/>
    <Button
            android:background="@drawable/confirm"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="360dp"
            android:layout_height="55dp" android:id="@+id/confirmButton"
            app:layout_constraintHorizontal_bias="0.515" app:layout_constraintVertical_bias="0.958"/>
</androidx.constraintlayout.widget.ConstraintLayout>